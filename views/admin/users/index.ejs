<%- include ('../../partials/header.ejs') %> 
<%- include ('../../partials/navbar.ejs') %> 

<div class="container">
    <hr>
    <h1>Usuários</h1>
    <a class="btn btn-success" href="/admin/users/create">Criar uma novo usuário</a>
    <hr>
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                   <td><%= user.id  %> </td>
                   <td><%= user.email  %> </td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</div>



<%- include ('../../partials/footer.ejs') %>